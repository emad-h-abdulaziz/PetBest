        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LoginWindow"
        mc:Ignorable="d"
        Title="View Service" Height="450" Width="796.8" Loaded="Window_Loaded" Background="#FF464A4F">
    <Grid>
        <ListBox Name="lstService" HorizontalAlignment="Left" Height="253" VerticalAlignment="Top" Width="728" Margin="33,25,0,0" FontFamily="Lucida Fax" Background="#FFEEEFF1"/>
        <Button Name="btnClose" Content="Cancel" HorizontalAlignment="Left" VerticalAlignment="Top" Width="107" Margin="405,322,0,0" Height="36" Click="btnClose_Click" FontFamily="Lucida Fax"/>
        <Button x:Name="btnSelect" Content="Select" HorizontalAlignment="Left" Margin="234,322,0,0" VerticalAlignment="Top" Width="106" Height="36" Click="BtnSelect_Click" FontFamily="Lucida Fax"/>

    </Grid>
</Window>
